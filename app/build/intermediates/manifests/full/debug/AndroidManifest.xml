    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.timetable.InitialScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.timetable.Register"
            android:label="@string/title_activity_login" />
        <activity
            android:name="com.example.timetable.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name="com.example.timetable.AddSubject"
            android:label="@string/title_activity_add_subject" />
        <activity
            android:name="com.example.timetable.AddSubActivity"
            android:label="@string/title_activity_add_subject" />
        <activity
            android:name="com.example.timetable.AddFacultyActivity"
            android:label="@string/title_activity_add_faculty" />
        <activity
            android:name="com.example.timetable.AddFaculty"
            android:label="@string/title_activity_add_faculty" />
        <activity
            android:name="com.example.timetable.GenerateActivity"
            android:label="@string/title_activity_add_subject" />
        <activity
            android:name="com.example.timetable.AssignActivity"
            android:label="@string/title_activity_add_faculty" />
        <activity
            android:name="com.example.timetable.StudentMainActivity"
            android:label="@string/title_activity_add_faculty" />
        <activity
            android:name="com.example.timetable.SessionDetails"
            android:label="@string/title_activity_main_activity2"
            android:screenOrientation="landscape" />
        <activity android:name="com.example.timetable.MainActivity" />
        <activity android:name="com.example.timetable.AddVideoLecturesActivity" />
        <activity android:name="com.example.timetable.SubjectVideosUploadActvity" />
        <activity android:name="com.example.timetable.LectureOrMaterialUploadActivity" />
        <activity android:name="com.example.timetable.ViewLectureVideosActivity" />
        <activity android:name="com.example.timetable.ViewSubjectVideosActivity" />
        <activity android:name="com.example.timetable.LectureOrMaterialVideosActivity" />
